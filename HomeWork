package com.company;

class Main {

    public static void main(String[] args) {

        int a = 785*231;
        System.out.println("a = 785*231 = "+a);
        float b = 4.3f*7.8f;
        System.out.println("b = 4.3f*7.8f = "+b);
        char lettx ='x';
        System.out.println("lettx = "+lettx);
        char letty ='y';
        System.out.println("letty = "+letty);
        short xs = 1, ys=2, zs=3;
        System.out.println("xs = "+xs+" ys = "+ys+" zs = "+zs);
        System.out.print("xs = ys+zs = ");
        System.out.println(ys+zs);

    }

}
